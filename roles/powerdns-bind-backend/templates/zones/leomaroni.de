$ORIGIN leomaroni.de.
@ 3600 IN SOA ns.leomaroni.de. noc.leomaroni.de. (
    2020091301 ; serial ;;; UPDATE THIS AFTER EACH EDIT!
    3600       ; refresh (1 hour)
    1800        ; retry (30 min)
    604800     ; expire (168 day)
    3600       ; minimum (1 hour)
    )

@ 3600 IN NS ns1.he.net.
@ 3600 IN NS ns2.he.net.
@ 3600 IN NS ns3.he.net.
@ 3600 IN NS ns4.he.net.
@ 3600 IN NS ns5.he.net.
;;@ 3600 IN NS ns

ns IN A {{ ansible_default_ipv4.address }}
ns IN AAAA {{ ansible_default_ipv6.address }}

;; MAIL
;;;; MX
nl 3600 IN MX 1 cetus.het.hel.fi.labcode.de.
ml 3600 IN MX 1 cetus.het.hel.fi.labcode.de.
services 3600 IN MX 1 cetus.het.hel.fi.labcode.de.
@ 3600 IN MX 1 cetus.het.hel.fi.labcode.de.

;;;; SRV
_autodiscover._tcp.nl 3600 IN SRV 1 10 443 cetus.het.hel.fi.labcode.de.
_autodiscover._tcp.ml 3600 IN SRV 1 10 443 cetus.het.hel.fi.labcode.de.
_autodiscover._tcp.services 3600 IN SRV 1 10 443 cetus.het.hel.fi.labcode.de.
_autodiscover._tcp 3600 IN SRV 1 10 443 cetus.het.hel.fi.labcode.de.

;;;; SPF
nl 3600 IN TXT "v=spf1 a mx ~all"
ml 3600 IN TXT "v=spf1 a mx ~all"
services 3600 IN TXT "v=spf1 a mx ~all"
@ 3600 IN TXT "v=spf1 a mx ~all"

;;;; DKIM
dkim._domainkey.nl 3600 IN TXT "v=DKIM1;k=rsa;t=s;s=email;"
dkim._domainkey.ml 3600 IN TXT "v=DKIM1;k=rsa;t=s;s=email;"
dkim._domainkey.services 3600 IN TXT "v=DKIM1;k=rsa;t=s;s=email;"
dkim._domainkey 3600 IN TXT "v=DKIM1;k=rsa;t=s;s=email;"

;;;; CNAME
autoconfig.nl 3600 IN CNAME cetus.het.hel.fi.labcode.de.
autodiscover.nl 3600 IN CNAME cetus.het.hel.fi.labcode.de.
autoconfig.ml 3600 IN CNAME cetus.het.hel.fi.labcode.de.
autodiscover.ml 3600 IN CNAME cetus.het.hel.fi.labcode.de.
autoconfig.services 3600 IN CNAME cetus.het.hel.fi.labcode.de.
autodiscover.services 3600 IN CNAME cetus.het.hel.fi.labcode.de.
autoconfig 3600 IN CNAME cetus.het.hel.fi.labcode.de.
autodiscover 3600 IN CNAME cetus.het.hel.fi.labcode.de.
mail 3600 IN CNAME cetus.het.hel.fi.labcode.de.

;; CAA
*	3600	IN	CAA	0 issue "letsencrypt.org"
@	3600	IN	CAA	0 issue "letsencrypt.org"

;; Pages
@ 3600 IN ALIAS dione.int.sig.de.labcode.de.
www 3600 IN CNAME dione.int.sig.de.labcode.de.
cloud 3600 IN CNAME dione.int.sig.de.labcode.de.
md 3600 IN CNAME dione.int.sig.de.labcode.de.
openpgpkey 3600 IN CNAME wkd.keys.openpgp.org.
