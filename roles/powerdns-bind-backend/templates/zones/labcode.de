$ORIGIN labcode.de.
@ 3600 IN SOA ns.labcode.de. noc.labcode.de. (
    2021031603 ; serial ;;; UPDATE THIS AFTER EACH EDIT!
    3600       ; refresh (1 hour)
    1800       ; retry (30 min)
    604800     ; expire (168 day)
    3600       ; minimum (1 hour)
    )

@ 3600 IN NS ns1.n0emis.eu.
@ 3600 IN NS ns2.n0emis.eu.
@ 3600 IN NS ns3.n0emis.eu.
@ 3600 IN NS ns.labcode.de.

dyn 300 IN NS ns1.n0emis.eu.
dyn 300 IN NS ns2.n0emis.eu.
dyn 300 IN NS ns3.n0emis.eu.
dyn 300 IN NS ns.labcode.de.

ns IN A {{ ansible_default_ipv4.address }}
ns IN AAAA {{ ansible_default_ipv6.address }}

;; Server
;;;; Cloud
cetus.het.hel.fi 3600 IN A 95.216.160.224
cetus.het.hel.fi 3600 IN AAAA 2a01:4f9:c010:1dcf::1
janus.ion.rhr.de 3600 IN A 93.90.205.65
janus.ion.rhr.de 3600 IN AAAA 2001:8d8:1800:30a::1

;;;; Home
titan.int.sig.de 60 IN CNAME titan.int.sig.de.dyn
*.titan.int.sig.de 3600 IN CNAME titan.int.sig.de

helene.int.sig.de 60 IN CNAME helene.int.sig.de.dyn
*.helene.int.sig.de 3600 IN CNAME helene.int.sig.de
nas.home 3600 IN CNAME encladus.int.sig.de
tethys.int.sig.de 3600 IN AAAA 2001:470:7285:2::2

; backwards compatibility for external DNS
dione.int.sig.de 3600 IN CNAME foros.int.sig.de.em0lar.dev.

;; MAIL
;;;; MX
infra 3600 IN MX 1 cetus.het.hel.fi
@ 3600 IN MX 1 cetus.het.hel.fi

@ 3600 IN MX 10 backupmx.dont-break.it.

;;;; SRV
_autodiscover.infra._tcp 3600 IN SRV 1 10 443 cetus.het.hel.fi
_autodiscover._tcp 3600 IN SRV 1 10 443 cetus.het.hel.fi
_matrix._tcp 3600 IN SRV 1 10 443 matrix

;;;; DKIM
dkim._domainkey 3600 IN TXT "v=DKIM1;k=rsa;t=s;s=email;"

;;;; SPF
infra 3600 IN TXT "v=spf1 a mx ~all"
@ 3600 IN TXT "v=spf1 a mx ~all"

;;;; CNAME
autoconfig.infra 3600 IN CNAME cetus.het.hel.fi
autoconfig 3600 IN CNAME cetus.het.hel.fi
autodiscover.infra 3600 IN CNAME cetus.het.hel.fi
autodiscover 3600 IN CNAME cetus.het.hel.fi
mail 3600 IN CNAME cetus.het.hel.fi
backupmx 3600 IN CNAME cetus.het.hel.fi

;; CAA
@	3600 IN CAA 0 issue "letsencrypt.org"
@ 3600 IN CAA 0 issue "buypass.no"
@ 3600 IN CAA 0 iodef "mailto:noc@labcode.de"

;; TXT-Verification
@ 3600 IN TXT "keybase-site-verification=73L68-5Q8Lcs8SFDUVoaGWCv4wCbOvDvyzd4Ip4-ab0"
_token._dnswl 3600 IN TXT "gsu43vu129t6z89sn1ohdchk9pspmm94"

;; Pages/Services
