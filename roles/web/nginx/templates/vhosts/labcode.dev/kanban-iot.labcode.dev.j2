server {
    listen 80;
    listen [::]:80;
    server_name kanban-iot.labcode.dev;
    {% include './templates/helpers/ssl_redirect.conf.j2' %}
}

server {
    listen 443 ssl http2;
    listen [::]:443 ssl http2;
    server_name kanban-iot.labcode.dev;

    {% set proxypass_url="http://localhost:40004" %}
    {% include './templates/helpers/proxypass.conf.j2' %}
    {% include './templates/helpers/general.conf.j2' %}
    {% include './templates/helpers/ssl_hsts.conf.j2' %}
    {% include './templates/helpers/ssl_domains_labcode_dev.conf.j2' %}
}