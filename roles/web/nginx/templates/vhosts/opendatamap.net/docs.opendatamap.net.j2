server {
    listen 80;
    listen [::]:80;
    server_name docs.opendatamap.net;
    root /var/www/opendatamap.net/docs.opendatamap.net/site;
    {% include './templates/helpers/ssl_redirect.conf.j2' %}
}

server {
    listen 443 ssl http2;
    listen [::]:443 ssl http2;
    server_name docs.opendatamap.net;

    root /var/www/opendatamap.net/docs.opendatamap.net/site;
    {% include './templates/helpers/general.conf.j2' %}
    {% include './templates/helpers/ssl_hsts.conf.j2' %}
    {% include './templates/helpers/ssl_domains_opendatamap_net.conf.j2' %}
}