server {
    listen 80;
    listen [::]:80;
    server_name cdn.labcode.de;
    root /var/www/labcode.de/cdn.labcode.de;
    {% include './templates/helpers/ssl_redirect.conf.j2' %}
}

server {
    listen 443 ssl http2;
    listen [::]:443 ssl http2;
    server_name cdn.labcode.de;

    root /var/www/labcode.de/cdn.labcode.de;
    add_header 'Access-Control-Allow-Origin' '*';
    {% include './templates/helpers/general.conf.j2' %}
    {% include './templates/helpers/ssl_hsts.conf.j2' %}
    {% include './templates/helpers/ssl_domains_labcode_de.conf.j2' %}
}